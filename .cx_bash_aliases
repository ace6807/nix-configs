export CDPATH='.:~:/opt/carsi'
export DBEDIT='/usr/local/bin/vim'

alias INSTALL='TINSTALL && REINSTALL && make cleanup F=ALL'
alias REINSTALL='make cleanup F=ALL && make REINSTALL F=ALL |& tee REINSTALL.Out'
alias TINSTALL='make cleanup F=ALL && make TINSTALL F=ALL |& tee TINSTALL.Out'
alias makeDeploy='cd CXConnect && make cleanup F=ALL && make tdeploy F=ALL && bundleproc -a && SU /sbin/init.d/CXConnect restart'

alias perlservs='cd /opt/carsi/web/cgi/services'
alias perlservsinst='cd /opt/apache22/cgi-bin/services'
alias perlmodules='cd /opt/apache22/cgi-bin/modules'
alias jservs='cd /opt/carsi/CXConnect/src'
alias carsdir='cd /opt/carsi/'
alias domaindir='cd /opt/carsi/CXConnect/application/domain/plugin'
alias schemadir='cd /opt/carsi/schema'
alias ifw='cd /opt/carsi/modules/compserv/'
alias s2='senter2'
